{
    "auto-build.defaultEnv.name": "sanguino1284p",
    "debug.allowBreakpointsEverywhere": true,
    "debug.openExplorerOnEnd": true,
    "debug.showInStatusBar": "never",
    "debug.toolBarLocation": "hidden",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.fontFamily": "MesloLGL Nerd Font",
    "editor.fontLigatures": false,
    "editor.guides.bracketPairs": "active",
    "editor.insertSpaces": true,
    "editor.lineNumbers": "on",
    "editor.rulers": [
        120
    ],
    "editor.tabCompletion": "on",
    "editor.tabSize": 4,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "keyword.other.dotenv",
                "settings": {
                    "foreground": "#FF000005"
                }
            }
        ]
    },
    "explorer.confirmDragAndDrop": false,
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "*.ftr": "apacheconf",
        "*.kml": "xml"
    },
    "files.watcherExclude": {
        "**/.git/**": true,
        "**/.venv/**": true,
        "**/node_modules/**": true
    },
    "gitlens.advanced.messages": {
        "suppressGitMissingWarning": true
    },
    "gitlens.hovers.currentLine.over": "line",
    "jupyter.askForKernelRestart": false,
    "jupyter.widgetScriptSources": [
        "jsdelivr.com",
        "unpkg.com"
    ],
    "latex-workshop.view.pdf.viewer": "tab",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "notebook.lineNumbers": "on",
    "partialDiff.enableTelemetry": false,
    "prettier.tabWidth": 4,
    "projects.configPath": "~/.vscode_projects.json",
    "projects.statusbarEnabled": false,
    "python.formatting.provider": "black",
    "python.linting.flake8Enabled": true,
    "redhat.telemetry.enabled": false,
    "remote.SSH.defaultExtensions": [
        "eamodio.gitlens",
        "mrmlnc.vscode-duplicate",
        "wraith13.bracket-lens",
        "oderwat.indent-rainbow",
        "aaron-bond.better-comments",
        "usernamehw.errorlens",
        "ryu1kn.partial-diff",
        "fabiospampinato.vscode-statusbar-debugger",
        "robbowen.synthwave-vscode",
        "mhutchie.git-graph"
    ],
    "search.exclude": {
        "**/.direnv*": true,
        "**/.git": true,
        "**/node_modules": true
    },
    "synthwave84.brightness": 0.75,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.fontFamily": "Fira Code",
    "terminal.integrated.scrollback": 100000000,
    "vim.vimrc.enable": true,
    "vim.vimrc.path": "~/.vimrc",
    "vsicons.dontShowNewVersionMessage": true,
    "window.commandCenter": true,
    "workbench.colorCustomizations": {
        "editorRuler.foreground": "#464451"
    },
    "workbench.colorTheme": "SynthWave '84",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.untitled.hint": "hidden",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.iconTheme": "vscode-icons",
    "workbench.startupEditor": "newUntitledFile"
}